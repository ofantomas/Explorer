{
  "env": [
    {
      "name": ["Pixelcopter-PLE-v0"],
      "max_episode_steps": [500],
      "input_type": ["feature"]
    }
  ],
  "agent": [
    {
      "name": ["MaxminDQN"],
      "target_networks_num": [10]
    }
  ],
  "train_steps": [2e6],
  "test_per_episodes": [-1],
  "device": ["cuda"],
  "hidden_layers": [[64,64]],
  "memory_type": ["Replay"],
  "memory_size": [1e4],
  "exploration_type": ["LinearEpsilonGreedy"],
  "exploration_steps": [1e3],
  "epsilon_steps": [1e3],
  "epsilon_start": [1.0],
  "epsilon_end": [0.01],
  "epsilon_decay": [0.999],
  "loss": ["MSELoss"],
  "optimizer": [
    {
      "name": ["RMSprop"],
      "kwargs": [{"lr": [3e-4]}]
    }
  ],
  "eta": [
    {
      "init_d": [2],
      "min_num_nets": [2],
      "update_d_interval": [100000],
      "update_d_gamma": [0.9999],
      "Q_G_eval_interval": [5],
      "Q_G_n_per_episode": [20],
      "Q_G_n_episodes": [200],
      "sampling_scheme": ["episodes"]
    }
  ],
  "batch_size": [32],
  "display_interval": [100],
  "rolling_score_window": [{"Train": [100], "Test": [10]}],
  "discount": [0.99],
  "seed": [10, 20, 30, 40, 50],
  "show_tb": [true],
  "gradient_clip": [5],
  "target_network_update_frequency": [200],
  "network_update_frequency": [1],
  "generate_random_seed": [false]
}